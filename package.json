{
    "name": "p-id",
    "version": "1.0.0",
    "description": "This is the backend microservice",
    "author": "Backend",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "npx nest start --watch",
        "start:debug": "npx nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "typeorm:migrate": "npm run typeorm  -- -d ./src/config/database.config.ts migration:generate ./src/migrations/$npm_config_name",
        "typeorm:run": "npm run typeorm migration:run -- -d ./src/config/database.config.ts",
        "typeorm:revert": "npm run typeorm -- -d ./src/config/database.config.ts migration:revert",
        "typeorm:create-migration": "npm run typeorm -- migration:create ./migrations/$npm_config_name"
    },
    "dependencies": {
        "@grpc/grpc-js": "^1.8.7",
        "@grpc/proto-loader": "^0.7.4",
        "@nestjs/axios": "^3.0.1",
        "@nestjs/common": "^9.2.1",
        "@nestjs/core": "^9.2.1",
        "@nestjs/jwt": "^10.2.0",
        "@nestjs/mapped-types": "^1.2.0",
        "@nestjs/microservices": "^9.2.1",
        "@nestjs/platform-express": "^9.2.1",
        "@nestjs/schedule": "^2.1.0",
        "@nestjs/swagger": "^6.1.4",
        "@nestjs/typeorm": "^9.0.1",
        "@types/morgan": "^1.9.3",
        "@types/multer": "^1.4.7",
        "async": "^3.2.4",
        "axios": "^1.6.5",
        "body-parser": "^1.20.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.14.0",
        "crypto-random-string": "^3.3.0",
        "cryptr": "^6.0.3",
        "dotenv": "^16.0.3",
        "eckey-util": "^1.0.4",
        "elastic-apm-node": "^3.41.1",
        "generate-password": "^1.7.0",
        "guid-typescript": "^1.0.9",
        "jest-extended": "^3.2.4",
        "jwt-decode": "^3.1.2",
        "latest-version": "^7.0.0",
        "moment-jalaali": "^0.10.0",
        "moment-timezone": "^0.5.40",
        "nest-router": "^1.0.9",
        "pg": "^8.8.0",
        "reflect-metadata": "^0.1.13",
        "request": "^2.88.2",
        "rimraf": "^3.0.2",
        "rxjs": "^7.8.0",
        "typeorm": "^0.3.11"
    },
    "devDependencies": {
        "@nestjs/cli": "^9.1.5",
        "@nestjs/schematics": "^9.0.3",
        "@nestjs/testing": "^9.2.1",
        "@types/async": "^3.2.18",
        "@types/express": "^4.17.15",
        "@types/jest": "^29.2.4",
        "@types/node": "^18.11.17",
        "@types/supertest": "^2.0.12",
        "eslint": "^8.30.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.3.1",
        "prettier": "^2.8.1",
        "supertest": "^6.3.3",
        "ts-jest": "^29.0.3",
        "ts-loader": "^9.4.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.1.1",
        "typescript": "^4.9.5"
    }
}
